{
  "compilerOptions": {
    "target": "ES5",
    "lib": ["dom", "es2015"],
    "jsx": "react-jsx",
    "moduleResolution": "node",
    "esModuleInterop": true,
    "strict": true,
    "skipLibCheck": true,
    "outDir": "./src/dist",
    "forceConsistentCasingInFileNames": true
  },
  "include": ["src/**/*"]
}
